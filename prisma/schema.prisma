generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  directUrl    = env("DIRECT_URL")
  relationMode = "prisma"
}

model User {
  id             String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  insta_username String   @default("") @db.VarChar
  full_name      String   @default("") @db.VarChar
  traits         String[] @db.VarChar
  persona        String   @unique
}

